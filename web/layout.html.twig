<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta name="description" content="{{sDescription}}">

    <title>{{sName}}</title>

    


<link rel="stylesheet" href="http://yui.yahooapis.com/pure/0.6.0/pure-min.css">


    <!--[if lte IE 8]>
        <link rel="stylesheet" href="css/layouts/email-old-ie.css">
    <![endif]-->
    <!--[if gt IE 8]><!-->
        <link rel="stylesheet" href="http://points.icomefromthenet.com/css/layouts/email.css">
    <!--<![endif]-->
  
    

</head>
<body>


<div id="layout" class="content pure-g">
    <div id="nav" class="pure-u">
        <a href="#" class="nav-menu-button">Menu</a>

        <div class="nav-inner">
            <div class="pure-menu">
                <ul class="pure-menu-list">
                    <li class="pure-menu-heading">Results</li>
                    <li class="pure-menu-item"><a href="#" class="pure-menu-link">Recent Runs </a></li>
                    <li class="pure-menu-item"><a href="#" class="pure-menu-link">New Donation</a></li>
                    <li class="pure-menu-item"><a href="#" class="pure-menu-link">New Raid</a></li>
                    
                    <li class="pure-menu-heading">Setup</li>
                    <li class="pure-menu-item"><a href="{{ path_for('setup_system',{}) }}" class="pure-menu-link">Point Systems</a></li>
                    <li class="pure-menu-item"><a href="#" class="pure-menu-link">Systems Zones</a></li>
                    
                    <li class="pure-menu-item"><a href="#" class="pure-menu-link">Event Types</a></li>
                    <li class="pure-menu-item"><a href="#" class="pure-menu-link">Scores</a></li>
                    <li class="pure-menu-item"><a href="#" class="pure-menu-link">Score Groups</a></li>
                    
                    
                    <li class="pure-menu-item"><a href="#" class="pure-menu-link">Rule Chains</a></li>
                    <li class="pure-menu-item"><a href="#" class="pure-menu-link">Rule Chains Members</a></li>
                    
                    <li class="pure-menu-item"><a href="#" class="pure-menu-link">Adjustment Rule Groups</a></li>
                    <li class="pure-menu-item"><a href="#" class="pure-menu-link">Adjustment Rules</a></li>
                    
                </ul>
            </div>
        </div>
    </div>

    {% block bodyContent %}

  
  
    {% endblock %}  
  
  
</div>

<script src="http://yui.yahooapis.com/3.17.2/build/yui/yui-min.js"></script>

<script>
    YUI().use('node-base', 'node-event-delegate', function (Y) {

        var menuButton = Y.one('.nav-menu-button'),
            nav        = Y.one('#nav');

        // Setting the active class name expands the menu vertically on small screens.
        menuButton.on('click', function (e) {
            nav.toggleClass('active');
        });

        // Your application code goes here...
        PAGE.init();
    });
</script>

{% block scriptExtras %}

    
{% endblock %}


</body>
</html>
